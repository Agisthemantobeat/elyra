{
  "name": "@elyra/python-runner-extension",
  "version": "0.4.0",
  "description": "JupyterLab extension - Run python files using a kernel runtime",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/elyra-ai/elyra",
  "bugs": {
    "url": "https://github.com/elyra-ai/elyra/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elyra-ai/elyra/"
  },
  "license": "Apache-2.0",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf node_modules && rimraf yarn.lock && rimraf yarn-error.log && rimraf tsconfig.tsbuildinfo",
    "dist": "npm pack .",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "~1.2.1",
    "@jupyterlab/apputils": "~1.2.1",
    "@jupyterlab/codeeditor": "~1.2.0",
    "@jupyterlab/docregistry": "~1.2.1",
    "@jupyterlab/fileeditor": "~1.2.1",
    "@jupyterlab/launcher": "~1.2.1",
    "@jupyterlab/mainmenu": "~1.2.1",
    "@jupyterlab/outputarea": "~1.2.2",
    "@phosphor/messaging": "~1.3.0",
    "@phosphor/widgets": "~1.9.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "typescript": "~3.5.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
