    "jest-junit": "^10.0.0",
    "ts-jest": "^24.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/jest": "^24.0.23",
    "rimraf": "~3.0.0",
    "typescript": "~3.7.3"
  }
}
